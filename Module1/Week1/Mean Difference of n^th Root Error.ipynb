    "def md_nre_single_sample2 (y , y_hat , n , p ) :\n",
    "    y_root = y ** (1/ n )\n",
    "    y_hat_root = y_hat ** (1/ n )\n",
    "    difference = y_root - y_hat_root\n",
    "    loss = difference ** p\n",
    "    return loss\n",
    "\n",
    "md_nre_single_sample2 (10, 9, 2, 2)"
   ]
  }
